#include<iostream>
using namespace std;
int main(){
	int a,b,c;
	cout<<"enter first:";
	cin>>a;
	cout<<"enter second:";
	cin>>b;
	cout<<"enter third:";
	cin>>c;
	if(a>b && a>c){
		cout<<"a is greatest";
	}
	else if(b>a && b>c){
		cout<<"b is greatest";
	}
	else{
		cout<<"c is greatest";
	}
	return 0;
}
